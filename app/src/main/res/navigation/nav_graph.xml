<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/news">

    <fragment
        android:id="@+id/news"
        android:name="com.theaverageguy.news.Ui.Fragment.News"
        android:label="@string/app_name"
        tools:layout="@layout/news_fragment">
        <action
            android:id="@+id/news_to_mWebView"
            app:destination="@id/mWebView" />
        <action
            android:id="@+id/home_to_setting"
            app:destination="@id/setting2" />
    </fragment>
    <fragment
        android:id="@+id/mWebView"
        android:name="com.theaverageguy.news.Views.mWebView"
        android:label="Article"
        tools:layout="@layout/mweb_view">
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/setting2"
        android:name="com.theaverageguy.news.Ui.Fragment.Setting"
        android:label="Setting"
        tools:layout="@layout/setting" />
</navigation>